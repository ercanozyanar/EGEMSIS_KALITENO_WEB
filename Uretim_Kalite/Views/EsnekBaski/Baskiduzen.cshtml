@using PagedList.Mvc;
@using PagedList;
@using Uretim_Kalite.Models.Entity;

@model PagedList.IPagedList<EGEM_ESNEK_BASKI_KALITE>

@{
    ViewBag.Title = "Baskiduzen";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
&nbsp;
<div class="alert alert-warning alert-dismissible fade show" role="alert">
    <strong>Esnek Baskı Kalite NOK Liste</strong>
</div>
&nbsp;

<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="/lib/bootstrap.min.css">
<script src="/lib/jquery-1.12.2.min.js"></script>
<script src="/lib/bootstrap.min.js"></script>
<p></p>
<div class="form-group row">
    &nbsp;&nbsp;
    <div>
        @using (Html.BeginForm("Baskiduzen", "EsnekBaski", FormMethod.Get))
        {
            <p>
                Tarih: @Html.TextBox("Search_Data", ViewBag.FilterValue as string)
                <input type="submit" value="Bul" />
            </p>
        }
    </div>
    &nbsp;&nbsp;
    <div>

        @using (Html.BeginForm("Export", "EsnekBaski", FormMethod.Post))
        {<input type="submit" value="Excel Aktar" class="btn-success" />}

    </div>
    &nbsp;&nbsp;
</div>
<table id="baskiduzen" class="table table-striped table-bordered table-hover " width="100" cellspacing="0">

    <tr>
        <th>SIL</th>
        <th>URUN_KODU</th>
        <th>URUN_ADI</th>
        <th>SIPARIS_NO</th>
        <th>ZAMAN</th>
        <th>BOBIN_NO</th>
        <th>HAMMADDE_KONTROL</th>
        <th>BOBIN_ENI</th>
        <th>FOTOSEL_ARALIGI</th>
        <th>NET_BASKI_ENI</th>
        <th>BASKI_YONU</th>
        <th>FILM_TIPI</th>
        <th>BARKOD_OKUNUR</th>
        <th>ACILIS_YONU</th>
        <th>ARKA_KAPANMA_SEKLI</th>
        <th>ICERIK_KONTROL</th>
        <th>RENK_UYGUN</th>
        <th>RENK_UYGUN_GOZ</th>
        <th>BASKI_KAYIK</th>
        <th>GORSEL_KONTROL</th>
        <th>ADEZYON</th>
        <th>KURUMA</th>
        <th>TONLAMA</th>
        <th>SAKAL</th>
        <th>CIZGI</th>
        <th>ISIL_YAPISMA</th>
        <th>MUREKKEP_MIKTAR</th>
        <th>BIRIM_AGIRLIK</th>
        <th>COF</th>
        <th>GC_TEST</th>
        <th>BOBIN_ICBAYRAK_SAYI</th>
        <th>MAKINE</th>
        <th>KONTROL_ONAY</th>
        <th>SILINDIR_KOD_KONTROL</th>
        <th>GLOSS</th>
    </tr>
    <tbody>
        @foreach (var ebaski in Model)
        {
            <tr>
                <th><a href="/EsnekBaski/Sil/@ebaski.ID" class="btn btn-danger">SIL</a></th>
                <th>@ebaski.URUN_KODU</th>
                <td>@ebaski.URUN_ADI</td>
                <td>@ebaski.SIPARIS_NO</td>
                <td>@ebaski.ZAMAN</td>
                <td>@ebaski.BOBIN_NO</td>
                <td>@ebaski.HAMMADDE_KONTROL</td>
                <td>@ebaski.BOBIN_ENI</td>
                <td>@ebaski.FOTOSEL_ARALIGI</td>
                <td>@ebaski.NET_BASKI_ENI</td>
                <td>@ebaski.BASKI_YONU</td>
                <td>@ebaski.FILM_TIPI</td>
                <td>@ebaski.BARKOD_OKUNUR</td>
                <td>@ebaski.ACILIS_YONU</td>
                <td>@ebaski.ARKA_KAPANMA_SEKLI</td>
                <td>@ebaski.ICERIK_KONTROL</td>
                <td>@ebaski.RENK_UYGUN</td>
                <td>@ebaski.RENK_UYGUN_GOZ</td>
                <td>@ebaski.BASKI_KAYIK</td>
                <td>@ebaski.GORSEL_KONTROL</td>
                <td>@ebaski.ADEZYON</td>
                <td>@ebaski.KURUMA</td>
                <td>@ebaski.TONLAMA</td>
                <td>@ebaski.SAKAL</td>
                <td>@ebaski.CIZGI</td>
                <td>@ebaski.ISIL_YAPISMA</td>
                <td>@ebaski.MUREKKEP_MIKTAR</td>
                <td>@ebaski.BIRIM_AGIRLIK</td>
                <td>@ebaski.COF</td>
                <td>@ebaski.GC_TEST</td>
                <td>@ebaski.BOBIN_ICBAYRAK_SAYI</td>
                <td>@ebaski.MAKINE</td>
                <td>@ebaski.KONTROL_ONAY</td>
                <td>@ebaski.SILINDIR_KOD_KONTROL</td>
                <td>@ebaski.GLOSS</td>
            </tr>
        }
    </tbody>

</table>

        Page @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber) of @Model.PageCount
        @Html.PagedListPager(Model, Page_No =>Url.Action("Baskiduzen",
            new { Page_No, Sorting_Order= ViewBag.CurrentSortOrder, Filter_Value = ViewBag.FilterValue }))




