<script type="text/javascript">
    $(document).ready(function () {
        $("#send").click(function () {
            var HATAIDX = ""
            var MSIPNO = $("#MSIPARIS_NO").val();
            var MBOBINNO = $("#MBOBINNO").val();
            var MHATA = $("#message-text").val();
            var HATAIDX = $("#HATAID").val();
            if (HATAIDX == "1") { $("#Nokgir1").val(MHATA) }
            if (HATAIDX == "2") { $("#Nokgir2").val(MHATA) }
            if (HATAIDX == "3") { $("#Nokgir3").val(MHATA) }
            if (HATAIDX == "4") { $("#Nokgir4").val(MHATA) }
            if (HATAIDX == "5") { $("#Gtxt5").val(MHATA) }
            if (HATAIDX == "6") { $("#Gtxt6").val(MHATA) }
            if (HATAIDX == "7") { $("#Gtxt7").val(MHATA) }
            if (HATAIDX == "8") { $("#Nokgir8").val(MHATA) }
            if (HATAIDX == "9") { $("#Nokgir9").val(MHATA) }
            if (HATAIDX == "10") { $("#Nokgir10").val(MHATA) }
            if (HATAIDX == "11") { $("#Nokgir11").val(MHATA) }
            if (HATAIDX == "12") { $("#Nokgir12").val(MHATA) }
            if (HATAIDX == "13") { $("#Nokgir13").val(MHATA) }
            if (HATAIDX == "14") { $("#Gtxt14").val(MHATA) }
            if (HATAIDX == "15") { $("#Gtxt15").val(MHATA) }
            if (HATAIDX == "16") { $("#Gtxt16").val(MHATA) }
            if (HATAIDX == "17") { $("#Nokgir17").val(MHATA) }
            if (HATAIDX == "18") { $("#Nokgir18").val(MHATA) }
            if (HATAIDX == "19") { $("#Nokgir19").val(MHATA) }
            if (HATAIDX == "20") { $("#Nokgir20").val(MHATA) }
            if (HATAIDX == "21") { $("#Gtxt21").val(MHATA) }
            if (HATAIDX == "22") { $("#Nokgir22").val(MHATA) }
            if (HATAIDX == "23") { $("#Nokgir23").val(MHATA) }
            if (HATAIDX == "24") { $("#Gtxt24").val(MHATA) }
            if (HATAIDX == "25") { $("#Gtxt25").val(MHATA) }
            if (HATAIDX == "26") { $("#Gtxt26").val(MHATA) }
            $.ajax({
                type: 'POST',
                url: '/OfsetBaski/SendEmail/' + MSIPNO, MBOBINNO,
                data: {
                    "MSIPARIS_NO": MSIPNO, "MBOBINNO": MBOBINNO, "MHATA": MHATA
                },
                datatype: "Json",
                success: function (data) { }
            });
            alert("Mesajınız Gönderilmiştir...")
            $('#exampleModal').modal('hide');
        });
    });
</script>
