@using PagedList.Mvc;
@using PagedList;
@using Uretim_Kalite.Models.Entity;

@model PagedList.IPagedList<EGEM_OFSET_BASKI_KALITE>
@{
    ViewBag.Title = "Ofsetbaskiduzen";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
&nbsp;
<div class="alert alert-warning alert-dismissible fade show" role="alert">
    <strong>Ofset Baskı Kalite NOK Liste</strong>
</div>
&nbsp;
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="/lib/bootstrap.min.css">
<script src="/lib/jquery-1.12.2.min.js"></script>
<script src="/lib/bootstrap.min.js"></script>
<p></p>
<div class="form-group row">
    &nbsp;&nbsp;
    <div>
        @using (Html.BeginForm("Ofsetbaskiduzen", "OfsetBaski", FormMethod.Get))
        {
            <p>
                Tarih: @Html.TextBox("Search_Data", ViewBag.FilterValue as string)
                <input type="submit" value="Bul" />
            </p>
        }
    </div>
    &nbsp;&nbsp;
    <div>
        @using (Html.BeginForm("Export", "OfsetBaski", FormMethod.Post))
        {<input type="submit" value="Excel Aktar" class="btn-success" />}
    </div>
    &nbsp;&nbsp;
</div>
<table id="baskiduzen" class="table table-striped table-bordered table-hover " width="100" cellspacing="0">
    <tr>
        <th>SIL</th>
        <th>URUN KODU</th>
        <th>URUN_ADI</th>
        <th>PROSES</th>
        <th>SIPARIS_NO</th>
        <th>TARIH</th>
        <th>SAAT</th>
        <th>MAKINE</th>
        <th>URUN ADI KONTROL</th>
        <th>PALET NO</th>
        <th>HAMMADDE KONTROL</th>
        <th>GRAMAJ KONTROL</th>
        <th>EBAT KONTROL</th>
        <th>ÇAPRAZ KONTROL</th>
        <th>YELPAZE KONTROL</th>
        <th>YAPISMA KONTROL</th>
        <th>SU YOLU KONTROL</th>
        <th>MUREKKEP KONTROL</th>
        <th>ARTWORK UYGUNLUK</th>
        <th>BIÇAK KONTROL</th>
        <th>POZA KONTROL</th>
        <th>TONLAMA SAKAL KONTROL</th>
        <th>GÖRSEL KONTROL</th>
        <th>LAK BOSALTMA KONTROL</th>
        <th>LAK AL KONTROL</th>
        <th>PARLAKLIK</th>
        <th>ASINMA</th>
        <th>COF KINETIK TEST</th>
        <th>COF STATIK TEST</th>
        <th>ISIL YAPISMA</th>
        <th>RENK KONTROL</th>
        <th>BARKOD KONTROL</th>
        <th>ASETAT KONTROL</th>
        <th>KORONA YONU</th>
        <th>SILIKONLU BANT</th>
        <th>KONTROL EDEN</th>
        <th>ACIKLAMA</th>
    </tr>
    <tbody>
        @foreach (var eobaski in Model)
        {
            <tr>
                <th><a href="/OfsetBaski/Sil/@eobaski.ID" class="btn btn-danger">SIL</a></th>
                <th>@eobaski.STOK_KODU</th>
                <td>@eobaski.STOK_ADI</td>
                <td>@eobaski.PROSES</td>
                <td>@eobaski.SIPARISNO</td>
                <td>@eobaski.TARIH</td>
                <td>@eobaski.SAAT</td>
                <td>@eobaski.CALISILAN_MKN</td>
                <td>@eobaski.URUN_ADI_KNT</td>
                <td>@eobaski.PALET_NO</td>
                <td>@eobaski.HAMMADDE_KONTROL</td>
                <td>@eobaski.GRAMAJ_KONTROL</td>
                <td>@eobaski.EBAT_KONTROL</td>
                <td>@eobaski.CAPRAZ_KONTROL</td>
                <td>@eobaski.YELPAZE_KONTROL</td>
                <td>@eobaski.YAPISMA_KONTROL</td>
                <td>@eobaski.SU_YOLU_KONTROL</td>
                <td>@eobaski.MUREKKEP_KONTROL</td>
                <td>@eobaski.ARTWORK_UYGUNLUK</td>
                <td>@eobaski.BICAK_KONTROL</td>
                <td>@eobaski.POZA_KONTROL</td>
                <td>@eobaski.TONLAMA_SAKAL_KONTROL</td>
                <td>@eobaski.GORSEL_KONTROL</td>
                <td>@eobaski.LAK_BOS_KONTROL</td>
                <td>@eobaski.LAK_AL_KONTROL</td>
                <td>@eobaski.PARLAKLIK</td>
                <td>@eobaski.ASINMA</td>
                <td>@eobaski.COF_KINETIK_TEST</td>
                <td>@eobaski.COF_STATIK_TEST</td>
                <td>@eobaski.ISIL_YAPISMA</td>
                <td>@eobaski.RENK_KONTROL</td>
                <td>@eobaski.BARKOD_KONTROL</td>
                <td>@eobaski.ASETAT_KONTROL</td>
                <td>@eobaski.KORONA_YONU</td>
                <td>@eobaski.SILIKONLU_BANT</td>
                <td>@eobaski.KONTROL_EDEN</td>
                <td>@eobaski.ACIKLAMA</td>
            </tr>
        }
    </tbody>

</table>

        Page @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber) of @Model.PageCount
        @Html.PagedListPager(Model, Page_No =>Url.Action("Ofsetbaskiduzen",
            new { Page_No, Sorting_Order= ViewBag.CurrentSortOrder, Filter_Value = ViewBag.FilterValue }))

