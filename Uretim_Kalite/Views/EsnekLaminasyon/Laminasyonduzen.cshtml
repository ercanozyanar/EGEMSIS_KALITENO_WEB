@using PagedList.Mvc;
@using PagedList;
@using Uretim_Kalite.Models.Entity;

@model PagedList.IPagedList<EGEM_ESNEK_LAMINASYON_KALITE>

@{

    ViewBag.Title = "Laminasyonduzen";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
&nbsp;
<div class="alert alert-warning alert-dismissible fade show" role="alert">
    <strong>Esnek Laminasyon Kalite NOK Liste</strong>
</div>
&nbsp;
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="/lib/bootstrap.min.css">
<script src="/lib/jquery-1.12.2.min.js"></script>
<script src="/lib/bootstrap.min.js"></script>
<p></p>
<div class="form-group row">
    &nbsp;&nbsp;
    <div>
        @using (Html.BeginForm("Laminasyonduzen", "EsnekLaminasyon", FormMethod.Get))
        {
            <p>
                Tarih: @Html.TextBox("Search_Data", ViewBag.FilterValue as string)
                <input type="submit" value="Bul" />
            </p>
        }
    </div>
    &nbsp;&nbsp;
    <div>

        @using (Html.BeginForm("Export", "EsnekLaminasyon", FormMethod.Post))
        {<input type="submit" value="Excel Aktar" class="btn-success" />}
    </div>
    &nbsp;&nbsp;
</div>
<table id="laminasyonduzen" class="table table-striped table-bordered table-hover " width="100" cellspacing="0">

    <tr>
        <th>SIL</th>
        <th>MAKINA</th>
        <th>URUN_KODU</th>
        <th>URUN_ADI</th>
        <th>SIPARIS_NO</th>
        <th>ZAMAN</th>
        <th>BOBIN NO</th>
        <th>BOBIN ENI</th>
        <th>FOTOSEL ARALIGI</th>
        <th>AL_FOLYO_YONU</th>
        <th>TUTKAL GRAMAJI</th>
        <th>BIRIM AGIRLIK</th>
        <th>YAPISMA KONTROLU</th>
        <th>GORSEL KONTROL</th>
        <th>BENCIK KONTROLU</th>
        <th>ISIL YAPISMA KONTROLU</th>
        <th>LAMINASYON KUVVETI</th>
        <th>COF TESTI</th>
        <th>KALITE ONAY</th>
    </tr>
    <tbody>
        @foreach (var elaminasyon in Model)
        {
            <tr>
                <th><a href="/EsnekLaminasyon/Sil/@elaminasyon.ID" class="btn btn-danger">SIL</a></th>
                <th>@elaminasyon.MAKINE</th>
                <th>@elaminasyon.URUN_KODU</th>
                <td>@elaminasyon.URUN_ADI</td>
                <td>@elaminasyon.SIPARIS_NO</td>
                <td>@elaminasyon.ZAMAN</td>
                <td>@elaminasyon.BOBIN_NO</td>
                <td>@elaminasyon.FOTOSEL_ARALIGI</td>
                <td>@elaminasyon.FOTOSEL_ARALIGI</td>
                <td>@elaminasyon.AL_FOLYO_YONU</td>
                <td>@elaminasyon.TUTKAL_GRAMAJ</td>
                <td>@elaminasyon.BIRIM_AGIRLIK</td>
                <td>@elaminasyon.YAPISMA_KONTROL</td>
                <td>@elaminasyon.GORSEL_KONTROL</td>
                <td>@elaminasyon.BENCIK_KONTROL</td>
                <td>@elaminasyon.ISIL_YAPISMA_KONT</td>
                <td>@elaminasyon.LAMINASYON_KUVVETI</td>
                <td>@elaminasyon.COF_TEST</td>
                <td>@elaminasyon.KALITE_ONAY</td>

            </tr>
        }
    </tbody>

</table>
        Page @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber) of @Model.PageCount
        @Html.PagedListPager(Model, Page_No =>Url.Action("Laminasyonduzen",
            new { Page_No, Sorting_Order= ViewBag.CurrentSortOrder, Filter_Value = ViewBag.FilterValue }))

