@using PagedList.Mvc;
@using PagedList;
@using Uretim_Kalite.Models.Entity;

@model PagedList.IPagedList<EGEM_OFSET_BITIRME_KALITE>

@{
    ViewBag.Title = "Ofsetbitirmeduzen";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
&nbsp;
<div class="alert alert-warning alert-dismissible fade show" role="alert">
    <strong>Ofset Bitirme Kalite NOK Liste</strong>
</div>
&nbsp;
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="/lib/bootstrap.min.css">
<script src="/lib/jquery-1.12.2.min.js"></script>
<script src="/lib/bootstrap.min.js"></script>
<p></p>
<div class="form-group row">
    &nbsp;&nbsp;
    <div>
        @using (Html.BeginForm("Ofsetbitirmeduzen", "OfsetBitirme", FormMethod.Get))
        {
            <p>
                Tarih: @Html.TextBox("Search_Data", ViewBag.FilterValue as string)
                <input type="submit" value="Bul" />
            </p>
        }
    </div>
    &nbsp;&nbsp;
    <div>
        @using (Html.BeginForm("Export", "OfsetBitirme", FormMethod.Post))
        {<input type="submit" value="Excel Aktar" class="btn-success" />}
    </div>
    &nbsp;&nbsp;
</div>
<table id="ofsetbitimeduzen" class="table table-striped table-bordered table-hover " width="100" cellspacing="0">
    <tr>
        <th>SIL</th>
        <th>SIPARIS NO</th>
        <th>STOK KODU</th>
        <th>STOK ADI</th>
        <th>PROSES</th>
        <th>TARIH</th>
        <th>SAAT</th>
        <th>MAKINE</th>
        <th>URUN ADI KONTROL</th>
        <th>PALET NO</th>
        <th>HAMMADDE KONTROL</th>
        <th>SU YOLU</th>
        <th>ARTWORK UYGUNLUK</th>
        <th>BIÇAK FILM KONTROL</th>
        <th>GORSEL KONTROL</th>
        <th>BARKOD KONTROL</th>
        <th>PILYAJ KONTROL</th>
        <th>KASIM PATLATMA</th>
        <th>KIRIM DIRENCI</th>
        <th>NIK YERLERI KONTROL</th>
        <th>EMBOSS DEBOSS KONTROL</th>
        <th>EMBOSS DEBOSS REGISTER</th>
        <th>EMBOSS DEBOSS KONTROL2</th>
        <th>TUM PZS KONTROL</th>
        <th>TS UYGUNLUK</th>
        <th>PALET GENEL KONTROL</th>
        <th>ACIKLAMA</th>
        <th>KONTROL EDEN</th>
        <th>BUNDLE YUKSEK</th>
    </tr>
    <tbody>
        @foreach (var eobitirme in Model)
        {
            <tr>
                <th><a href="/OfsetBitirme/Sil/@eobitirme.ID" class="btn btn-danger">SIL</a></th>
                <th>@eobitirme.SIPARISNO</th>
                <td>@eobitirme.STOK_KODU</td>
                <td>@eobitirme.STOK_ADI</td>
                <td>@eobitirme.PROSES</td>
                <td>@eobitirme.TARIH</td>
                <td>@eobitirme.SAAT</td>
                <td>@eobitirme.CALISILAN_MKN</td>
                <td>@eobitirme.URUN_ADI_KNT</td>
                <td>@eobitirme.PALET_NO</td>
                <td>@eobitirme.HAMMADDE_KONTROL</td>
                <td>@eobitirme.SU_YOLU</td>
                <td>@eobitirme.ARTWORK_UYGUNLUK</td>
                <td>@eobitirme.BICAK_FILM_KONTROL</td>
                <td>@eobitirme.GORSEL_KONTROL</td>
                <td>@eobitirme.BARKOD_KONTROL</td>
                <td>@eobitirme.PILYAJ_KONTROL</td>
                <td>@eobitirme.KASIM_PATLATMA</td>
                <td>@eobitirme.KIRIM_DIRENCI</td>
                <td>@eobitirme.NIK_YERLERI_KONTROL</td>
                <td>@eobitirme.EMBOSS_DEBOSS_KONTROL</td>
                <td>@eobitirme.EMBOSS_DEBOSS_REGISTER</td>
                <td>@eobitirme.EMBOSS_DEBOSS_KONTROL2</td>
                <td>@eobitirme.TUM_PZS_KONT</td>
                <td>@eobitirme.TS_UYGUNLUK</td>
                <td>@eobitirme.PALET_GENEL_KONT</td>
                <td>@eobitirme.ACIKLAMA</td>
                <td>@eobitirme.KONTROL_EDEN</td>
                <td>@eobitirme.BUNDLE_YUKSEK</td>
            </tr>
        }
    </tbody>
</table>

        Page @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber) of @Model.PageCount
        @Html.PagedListPager(Model, Page_No =>Url.Action("Ofsetbitirmeduzen",
            new { Page_No, Sorting_Order= ViewBag.CurrentSortOrder, Filter_Value = ViewBag.FilterValue }))



