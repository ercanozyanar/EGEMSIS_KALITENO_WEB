                    <input class="form-control input-sm" type="text" id="Gtxt15" autocomplete="off" name="KIRIM_DIRENCI" value="">
                    &nbsp;&nbsp;
                    <button type="button" class="btn btn btn-danger" name="KIRIM_DIRENCI2" id="Nokgir15" value="15" data-toggle="modal" data-target="#exampleModal">NOTOK</button>

