@model IEnumerable<Uretim_Kalite.Models.Entity.EGEM_GRAVUR_KALITE_SIPARIS>
@{

    ViewBag.Title = "Ofsetbitirmeekle";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
&nbsp;
<div class="alert alert-warning alert-dismissible fade show" role="alert">
    <strong>Ofset Bitirme Kalite NOK Girişi</strong>
</div>
&nbsp;
<form class="form-group" method="post">
    <div class="form-group row">
        &nbsp;&nbsp;
        <div>
            <label for="ltxt1">SIPARIS NO</label>
            <input class="form-control input-sm" id="txt1" type="text" autocomplete="off" name="SIPARISNO" value="" size="20">
            <p></p>
            <input type="button" id="Sipbul" class="btn btn-danger" value="Sipariş Bul" />

        </div>
        &nbsp;&nbsp;
        <div>
            <label for="txt2">URUN KODU</label>
            <input class="form-control input-sm" id="skod_txt" type="text" readonly="readonly" name ="STOK_KODU" value="" size="20">
        </div>
        &nbsp;&nbsp;
        <div>
            <label for="txt3">URUN ADI</label>
            <input class="form-control input-sm" id="sad_txt" type="text" readonly="readonly" name="STOK_ADI" value="" size="80">
        </div>
    </div>

    <div class="form-group row">
        &nbsp;&nbsp;
        <div>
            <label for="drp1">MAKINE ADI</label>
            <fieldset>
                <select id="drp1" name="CALISILAN_MKN" class="form-select form-select-lg mb-3" aria-label=".form-select-lg example">
                    <option selected="selected" value="1">Makine Adı Seçiniz</option>
                    <option>106LER</option>
                    <option>BOBST_102E</option>
                    <option>106PER (EXPERCUT)</option>
                    <option>EXPERTFOIL</option>
                    <option>FOILMASTER</option>
                </select>
            </fieldset>
        </div>
        &nbsp;&nbsp;&nbsp;&nbsp;
        <div>
            <label for="drp1">KALITE ONAY</label>
            <br />
            @Html.DropDownList("KONTROL_EDEN", "Onay Veren Personel Seçiniz...")

        </div>
        &nbsp;&nbsp;&nbsp;&nbsp;
        <div>
            <label for="ltxt4">PALET NO</label>
            <input class="form-control input-sm" id="txt4" type="text" autocomplete="off" name="PALET_NO" size="10" />
        </div>
    </div>
    <table class="table table-striped table-bordered dt-responsive nowrap" width="100" cellspacing="0">
        <tr style="background-color:lemonchiffon">
            <th>Ürün Adı / Kodu Kontrolü</th>
            <th>
                <div class="btn-group" data-toggle="buttons">
                    <label class="btn btn-default alert-success"><input type="radio" name="URUN_ADI_KNT" value="OK">OK</label>
                    <label class="btn btn-default alert-danger"><input type="radio" name="URUN_ADI_KNT" value="1" id="Nokgir1" data-toggle="modal" data-target="#exampleModal"><span class="glyphicon"></span> NOT OK</label>
                </div>
            </th>
        </tr>
        <tr style="background-color:white">
            <th>Hammadde Kontrolü</th>
            <th>
                <div class="btn-group" data-toggle="buttons">
                    <label class="btn btn-default alert-success"><input type="radio" name="HAMMADDE_KONTROL" value="OK">OK</label>
                    <label class="btn btn-default alert-danger"><input type="radio" name="HAMMADDE_KONTROL" value="2" id="Nokgir2" data-toggle="modal" data-target="#exampleModal"><span class="glyphicon"></span> NOT OK</label>
                </div>
            </th>
        </tr>
        <tr style="background-color:lemonchiffon">
            <th>Su Yolu</th>
            <th>
                <div class="btn-group" data-toggle="buttons">
                    <label class="btn btn-default alert-success"><input type="radio" name="SU_YOLU" value="OK">OK</label>
                    <label class="btn btn-default alert-danger"><input type="radio" name="SU_YOLU" value="3" id="Nokgir3" data-toggle="modal" data-target="#exampleModal"><span class="glyphicon"></span> NOT OK</label>
                </div>
            </th>
        </tr>
        <tr style="background-color:white">
            <th>Tüm Pozisyonların Kontrolü</th>
            <th>
                <div class="btn-group" data-toggle="buttons">
                    <label class="btn btn-default alert-success"><input type="radio" name="TUM_PZS_KONT" value="OK">OK</label>
                    <label class="btn btn-default alert-danger"><input type="radio" name="TUM_PZS_KONT" value="4" id="Nokgir4" data-toggle="modal" data-target="#exampleModal"><span class="glyphicon"></span> NOT OK</label>
                </div>
            </th>
        </tr>
        <tr style="background-color:lemonchiffon">
            <th>Artwork'e Uygunluk</th>
