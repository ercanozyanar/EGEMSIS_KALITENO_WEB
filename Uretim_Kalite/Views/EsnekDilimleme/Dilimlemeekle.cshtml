                    <input class="form-control input-sm" id="Gtxt11" type="text" autocomplete="off" name="CIZGI" value="">
                    &nbsp;&nbsp;
                    <button type="button" class="btn btn btn-danger" name="CIZGI2" id="Nokgir11" value="11" data-toggle="modal" data-target="#exampleModal">NOTOK</button>
                </div>
            </th>
        </tr>
        <tr style="background-color:white">
            <th>Renk Uygunluğu</th>
            <th>
                <div class="btn-group" data-toggle="buttons">
                    <label class="btn btn-default alert-success"><input type="radio" name="RENK_UYGUN" value="OK">OK</label>
                    <label class="btn btn-default alert-danger"><input type="radio" name="RENK_UYGUN" id="Nokgir12" value="12" data-toggle="modal" data-target="#exampleModal"><span class="glyphicon"></span> NOT OK</label>
                </div>
            </th>
        </tr>
        <tr style="background-color:lemonchiffon">
            <th>Sarım Düzgünlüğü</th>
            <th>
