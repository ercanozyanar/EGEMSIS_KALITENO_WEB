@model IEnumerable<Uretim_Kalite.Models.Entity.EGEM_GRAVUR_KALITE_SIPARIS>

@{
    ViewBag.Title = "Dilimlemeekle";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
&nbsp;
<div class="alert alert-warning alert-dismissible fade show" role="alert">
    <strong>Esnek Dilimleme Kalite NOK Girişi</strong>
</div>
&nbsp;
<form class="form-group" method="post">
    <div class="form-group row">
        &nbsp;&nbsp;
        <div>
            <label for="ltxt1">SIPARIS NO</label>
            <input class="form-control input-sm" id="txt1" type="text" autocomplete="off" name="SIPARIS_NO" value="" size="20">
            <p></p>
            <input type="button" class="btn btn-danger" id="Sipbul" value="Sipariş Bul" />
        </div>
        &nbsp;&nbsp;
        <div>
            <label for="txt2">URUN KODU</label>
            <input class="form-control input-sm" id="skod_txt" type="text" readonly="readonly" name="URUN_KODU" value="" size="20">
        </div>
        &nbsp;&nbsp;
        <div>
            <label for="txt3">URUN ADI</label>
            <input class="form-control input-sm" id="sad_txt" type="text" readonly="readonly" name="URUN_ADI" value="" size="80">
        </div>
    </div>

    <div class="form-group row">
        &nbsp;&nbsp;
        <div>
            <label for="drp1">MAKINE ADI</label>
            <fieldset>
                <select id="drp1" name="MAKINE" class="form-select form-select-lg mb-3" aria-label=".form-select-lg example">
                    <option selected="selected" value="1">Makine Adı Seçiniz</option>
                    <option>TITAN</option>
                    <option>REVOMAC</option>
                </select>
            </fieldset>
        </div>

        &nbsp;&nbsp;&nbsp;&nbsp;
        <div>
            <label for="drp1">KALITE ONAY</label>
            <br />
            @Html.DropDownList("KALITE_ONAY", "Onay Veren Personel Seçiniz...")

        </div>
        &nbsp;&nbsp;&nbsp;&nbsp;
        <div>
            <label for="ltxt4">BOBIN NO</label>
            <input class="form-control input-sm" id="txt4" autocomplete="off" type="text" name="BOBIN_NO" size="10" />
        </div>
    </div>
    <table class="table table-striped table-bordered dt-responsive nowrap" width="100" cellspacing="0">
        <tr style="background-color:lemonchiffon">
            <th>Dilme Eni (mm) (Teknik Şartnameye uygunluk)</th>
            <th>
                <div class="btn-group" data-toggle="buttons">
                    <input class="form-control input-sm" id="Gtxt1" autocomplete="off" type="text" name="DILME_ENI">
                    &nbsp;&nbsp;
                    <button type="button" class="btn btn btn-danger" name="DILME_ENI2" id="Nokgir1" value="1" data-toggle="modal" data-target="#exampleModal">NOTOK</button>

                </div>
            </th>
        </tr>
        <tr style="background-color:white">
            <th>Fotosel Aralığı (mm)</th>
            <th>
                <div class="btn-group" data-toggle="buttons">
                    <input class="form-control input-sm" id="Gtxt2" autocomplete="off" type="text" name="FOTOSEL_ARALIGI" value="">
                    &nbsp;&nbsp;
                    <button type="button" class="btn btn btn-danger" name="FOTOSEL_ARALIGI2" id="Nokgir2" value="2" data-toggle="modal" data-target="#exampleModal">NOTOK</button>
                </div>
            </th>
        </tr>
        <tr style="background-color:lemonchiffon">
            <th>Açılış Yönü</th>
            <th>
                <div class="btn-group" data-toggle="buttons">
                    <label class="btn btn-default alert-success"><input type="radio" name="ACILIS_YONU" value="OK">OK</label>
                    <label class="btn btn-default alert-danger"><input type="radio" name="ACILIS_YONU" value="3" id="Nokgir3" data-toggle="modal" data-target="#exampleModal"><span class="glyphicon"></span> NOT OK</label>
                </div>
            </th>
        </tr>
        <tr style="background-color:white">
            <th>Masura İç Çapı (mm) (Teknik Şartnameye uygunluk)</th>
            <th>
                <div class="btn-group" data-toggle="buttons">
                    <label class="btn btn-default alert-success"><input type="radio" name="IC_CAP" value="OK">OK</label>
                    <label class="btn btn-default alert-danger"><input type="radio" name="IC_CAP" id="Nokgir4" value="4" data-toggle="modal" data-target="#exampleModal"><span class="glyphicon"></span> NOT OK</label>
                </div>
            </th>
        </tr>
        <tr style="background-color:lemonchiffon">
            <th>Ek Sayısı  (Teknik Şartnameye uygunluk)</th>
            <th>
                <div class="btn-group" data-toggle="buttons">
                    <label class="btn btn-default alert-success"><input type="radio" name="EK_SAYI" value="OK">OK</label>
                    <label class="btn btn-default alert-danger"><input type="radio" name="EK_SAYI" value="5" id="Nokgir5" data-toggle="modal" data-target="#exampleModal"><span class="glyphicon"></span> NOT OK</label>

                </div>
            </th>
        </tr>
        <tr style="background-color:white">
            <th>Bobin Çapı (mm)</th>
            <th>
                <div class="btn-group" data-toggle="buttons">
                    <input class="form-control input-sm" id="Gtxt6" type="text" autocomplete="off" name="BOBIN_CAP" value="">
                    &nbsp;&nbsp;
                    <button type="button" class="btn btn btn-danger" name="BOBIN_CAP2" id="Nokgir6" value="6" data-toggle="modal" data-target="#exampleModal">NOTOK</button>
                </div>
            </th>
        </tr>
        <tr style="background-color:lemonchiffon">
            <th>Görsel Kontrol</th>
            <th>
                <div class="btn-group" data-toggle="buttons">
                    <label class="btn btn-default alert-success"><input type="radio" name="GORSEL_KONTROL" value="OK">OK</label>
                    <label class="btn btn-default alert-danger"><input type="radio" name="GORSEL_KONTROL" value="7" id="Nokgir7" data-toggle="modal" data-target="#exampleModal"><span class="glyphicon"></span> NOT OK</label>
                </div>
            </th>
        </tr>
        <tr style="background-color:white">
            <th>Kesim Kayıklığı</th>
            <th>
                <div class="btn-group" data-toggle="buttons">
                    <label class="btn btn-default alert-success"><input type="radio" name="KESIM_KAYIK" value="OK">OK</label>
                    <label class="btn btn-default alert-danger"><input type="radio" name="KESIM_KAYIK" id="Nokgir8" value="8" data-toggle="modal" data-target="#exampleModal"><span class="glyphicon"></span> NOT OK</label>
                </div>
            </th>
        </tr>
        <tr style="background-color:lemonchiffon">
            <th>Tonlama</th>
            <th>
                <div class="btn-group" data-toggle="buttons">
                    <label class="btn btn-default alert-success"><input type="radio" name="TONLAMA" value="OK">OK</label>
                    <label class="btn btn-default alert-danger"><input type="radio" name="TONLAMA" value="9" id="Nokgir9" data-toggle="modal" data-target="#exampleModal"><span class="glyphicon"></span> NOT OK</label>
                </div>
            </th>
        </tr>
        <tr style="background-color:white">
            <th>Sakal</th>
            <th>
                <div class="btn-group" data-toggle="buttons">
                    <label class="btn btn-default alert-success"><input type="radio" name="SAKAL" value="OK">OK</label>
                    <label class="btn btn-default alert-danger"><input type="radio" name="SAKAL" id="Nokgir10" value="10" data-toggle="modal" data-target="#exampleModal"><span class="glyphicon"></span> NOT OK</label>
                </div>
            </th>
        </tr>
        <tr style="background-color:lemonchiffon">
            <th>Çizgi</th>
            <th>
